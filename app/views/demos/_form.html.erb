<%= form_with model: demo do |form| %>


<div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
</div>

<div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
</div>

<div>
    <%= form.label :description %><br>
    <%= form.text_field :description %>
</div>

<div>
    <%= form.submit %>
</div>

<% end %>